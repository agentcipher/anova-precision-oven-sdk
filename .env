# ============================================================================
# .env - Environment Variables (Alternative to .secrets.yaml)
# ============================================================================
# Place this in your project root and add to .gitignore

# Required
ANOVA_TOKEN=anova-your-token-here

# Optional overrides
ANOVA_ENV=development
ANOVA_LOG_LEVEL=DEBUG
ANOVA_CONNECTION_TIMEOUT=30.0
ANOVA_MAX_RETRIES=3
ANOVA_LOG_FILE=logs/anova.log